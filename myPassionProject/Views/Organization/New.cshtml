
@{
    ViewBag.Title = "Create";
}

<h2>Create Organization</h2>
<form action="/Organization/Create" method="post">

    <div>
        <label>Name:</label> <input type="text" name="OrganizationName" required/>
    </div>
    <div>
        <label>Contact:</label> <input type="text" name="OrganizationContact" required/>
    </div>
 
    <div>
        <input type="submit" value="Add Organization" />
    </div>
</form>

<a href="/Organization/List">Back to List</a>